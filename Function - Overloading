#include <stdio.h>
#include <iostream>
#include <iomanip>
using namespace std;

int square(int);
double square(double);

int main()
{
	int userInt;
	double userFloat;
	cout << fixed << setprecision(2);
	cout << "Enter an integer and a FP value: ";
	cin >> userInt >> userFloat;
	cout << "Here are the squares: ";
	cout << square(userInt) << " and " << square(userFloat);
return 0;
}
int square(int number)
{
	return number*number;
}

double square(double num)
{
    return num*num;
}

